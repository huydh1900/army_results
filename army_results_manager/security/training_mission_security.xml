<odoo>
    <!--    Cán bộ phụ trách hl chỉ xem những nhệm vụ huấn luyện mà họ được gắn tên-->
    <record id="training_mission_rule_commander_assistant" model="ir.rule">
        <field name="name">Cán bộ phụ trách hl chỉ xem những nhệm vụ huấn luyện mà họ được gắn tên</field>
        <field name="model_id" ref="army_results_manager.model_training_mission"/>
        <field name="groups" eval="[(4, ref('army_results_manager.group_training_commander_assistant'))]"/>
        <field name="domain_force">[('training_officer_ids.user_id', '=', user.id)]</field>
        <field name="perm_read">1</field>
        <field name="perm_write">1</field>
    </record>

</odoo>