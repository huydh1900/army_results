<odoo>
    <record id="view_training_mission_result_tree" model="ir.ui.view">
        <field name="name">training.mission.result.tree</field>
        <field name="model">training.mission.result</field>
        <field name="arch" type="xml">
            <tree editable="bottom" >
                <field name="student_id" readonly="1"/>
                <field name="score"/>
                <field name="note"/>
            </tree>
        </field>
    </record>

</odoo>